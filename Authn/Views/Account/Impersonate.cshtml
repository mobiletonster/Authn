@using Authn.Data
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
    List<AppUser> users = Model as List<AppUser>;
}
<h1>Impersonate</h1>
<ul>
@foreach(var u in users){
    <li><a href="/impersonate/@u.UserId">@u.Firstname @u.Lastname - @u.Provider</a></li>
}
</ul>